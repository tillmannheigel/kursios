<h1>New Student</h1>
<%= form_tag create_student_as_admin_path, :html => {:class =>"form-horizontal"} do %> 
<div class="form-group">
	<%= label_tag :email %>
	<%= text_field_tag :email,nil, :class => "form-control" %>
</div>
<div class="form-group">
	<%= submit_tag "Create", class: "btn btn-default" %>
	<%= link_to "Cancel",  students_path %>
</div>
<% end %>